{
  "name": "eleventy-plugin-json-feed",
  "description": "An Eleventy plugin for generating a JSON Feed using the Nunjucks templating engine.",
  "homepage": "https://git.genehack.net/genehack/eleventy-plugin-json-feed",
  "version": "0.0.6",
  "license": "MIT",
  "author": "John SJ Anderson <john@genehack.org>",
  "keywords": [
    "eleventy",
    "eleventy-plugin",
    "jsonfeed"
  ],
  "repository": {
    "type": "git",
    "url": "https://git.genehack.net/genehack/eleventy-plugin-json-feed"
  },
  "main": "index.js",
  "scripts": {
    "cover": "nyc mocha",
    "html-cover": "nyc --reporter=html mocha && open coverage/index.html",
    "test": "mocha"
  },
  "lint-staged": {
    "*.js": [
      "prettier --list-different",
      "eslint"
    ],
    "*.json": [
      "prettier --list-different"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "blockBindings": true
      }
    },
    "env": {
      "es6": true,
      "mocha": true,
      "node": true
    }
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.1.0",
    "husky": "^4.2.5",
    "jsonfeed-validator": "^0.2.0",
    "lint-staged": "^10.2.6",
    "mocha": "^7.2.0",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "is-url": "^1.2.4",
    "posthtml": "^0.13.0",
    "posthtml-urls": "^1.0.0",
    "striptags": "^3.1.1",
    "url": "^0.11.0"
  }
}
